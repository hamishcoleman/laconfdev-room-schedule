<!DOCTYPE html>
<html lang="en" style="font-size: 2.2em">
<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Everything Open 2024 | Up Next</title>

  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
  <link href="https://2024.everythingopen.au/static/build/scss/app.css" rel="stylesheet" type="text/css" />
  
  <script type="text/javascript">
    var CONF_TZ = "Australia/Brisbane";
  </script>

</head>
<body class="">
  <header class="clearfix d-print-none">

<nav class="navbar navbar-expand-lg navbar-light bg-dawn-sea">
  <div class="container">
    <a class="navbar-brand" href="/">
      <img src="https://2024.everythingopen.au/static/build/img/eo2024.svg" alt="Everything Open 2024 logo" height="56px">
    </a>

  </div>
  <div class="container">
    <h4>Up next</h4>
  </div>
</nav>

  </header>

  <main role="main">
    <table>
        <tr>
            <td width=10%>
                &nbsp;
            <td width=80%>
    <table border=1>
      <tr><th>When <th>Room <th>Name
{%   for room in ['turtle', 'emu', 'echidna', 'pelican'] %}
      <tr>
        <td><span class="start-time" data-starttime="{{ next[room].data["start"] }}">{{ next[room].data["start"] }}</span>
        <td><p>{{ next[room].data["room"] }}</p>
        <td>
          <p>{{ next[room].data["name"] }} ({{ next[room].author_name() }})</p>
{% endfor %}
    </table>
            <td width=10%>
                &nbsp;
    </table>
    
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://2024.everythingopen.au/static/build/js/app.js" type="text/javascript"></script>
    <script src="https://2024.everythingopen.au/static/build/js/jquery.formset.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://2024.everythingopen.au/static/build/bootstrap/js/bootstrap.bundle.min.js"></script>
    
    
<script src="https://2024.everythingopen.au/static/build/js/luxon.min.js"></script>
<script src="https://2024.everythingopen.au/static/build/js/schedule.js" type="text/javascript"></script>

<script>
    function renderStartTimes() {
        var presentationTimes = $('span.start-time');
        for (var i = 0; i < presentationTimes.length; ++i) {
            var presentationTime = $(presentationTimes[i]);
            var startTime = presentationTime.data('starttime');
            var confStartTime = luxon.DateTime.fromISO(startTime, { zone: CONF_TZ });

            var confStartTimeFormatted = confStartTime.toLocaleString({
                weekday: 'short',
                month: 'short',
                day: '2-digit',
                hour: 'numeric',
                minute: 'numeric',
            });

            presentationTime.html(confStartTimeFormatted);
        }
    }

renderStartTimes()

</script>

  </main>

</body>
</html>
